{
  "author": "azie abdullah <finetoo97@gmail.com>",
  "name": "nuxt3-editorjs",
  "version": "0.0.2",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint . --max-warnings=0",
    "types": "tsc --noEmit",
    "build:module": "nuxi build",
    "build:playground": "nuxi build playground",
    "start:playground": "node playground/.output/server/index.mjs"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.4",
    "cheerio": "^1.0.0-rc.12",
    "defu": "^6.1.3",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@editorjs/attaches": "^1.3.0",
    "@editorjs/checklist": "^1.5.0",
    "@editorjs/code": "^2.8.0",
    "@editorjs/delimiter": "^1.3.0",
    "@editorjs/editorjs": "^2.28.2",
    "@editorjs/embed": "^2.6.0",
    "@editorjs/header": "^2.7.0",
    "@editorjs/image": "^2.8.2",
    "@editorjs/inline-code": "^1.4.0",
    "@editorjs/link": "^2.5.0",
    "@editorjs/marker": "^1.3.0",
    "@editorjs/nested-list": "^1.3.0",
    "@editorjs/paragraph": "^2.11.3",
    "@editorjs/personality": "^2.0.2",
    "@editorjs/quote": "^2.5.0",
    "@editorjs/raw": "^2.4.0",
    "@editorjs/table": "^2.2.2",
    "@editorjs/text-variant-tune": "^1.0.1",
    "@editorjs/warning": "^1.3.0",
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/editorjs__header": "^2.6.2",
    "@types/multer": "^1.4.9",
    "editorjs-change-case": "^0.0.3",
    "editorjs-hyperlink": "^1.0.6",
    "editorjs-text-color-plugin": "^2.0.4",
    "editorjs-undo": "^2.0.25",
    "eslint": "^8.52.0",
    "nuxt": "^3.8.0"
  }
}
